{
  "name": "@guanghechen/venus",
  "version": "1.1.11",
  "description": "Merge cpp codes into single sources file.",
  "author": {
    "name": "guanghechen",
    "url": "https://github.com/guanghechen/"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:guanghechen/venus.git"
  },
  "homepage": "https://github.com/guanghechen/venus#readme",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">= 14.15.0"
  },
  "scripts": {
    ":publish": "yarn test:coverage && lerna publish",
    ":upgrade": "yarn upgrade-interactive && run-s clean format build test",
    "build": "lerna run --concurrency 1 build",
    "clean": "lerna clean --yes && yarn install",
    "format": "run-s format:lintfix format:prettier",
    "format:lintfix": "eslint . --fix",
    "format:prettier": "prettier --write .",
    "prepublishOnly": "pinst --disable",
    "postinstall": "run-p postinstall:dev",
    "postinstall:dev": "is-ci || husky install",
    "postpublish": "pinst --enable",
    "rebuild": "yarn clean && yarn build",
    "test": "lerna run test --concurrency 1",
    "test:coverage": "lerna run --concurrency 1 test -- --coverage",
    "test:update": "lerna run --concurrency 1 test -- -u",
    "update:changelog": "gitmoji-changelog update",
    "version": "yarn && git stage yarn.lock"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@guanghechen/eslint-config": "^1.9.5",
    "@guanghechen/eslint-config-jsx": "^1.9.5",
    "@guanghechen/eslint-config-ts": "^1.9.5",
    "@guanghechen/jest-config": "^1.9.5",
    "@guanghechen/jest-helper": "^1.9.5",
    "@guanghechen/rollup-config-cli": "^1.9.5",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.2.1",
    "@types/jest": "27.0.2",
    "@types/js-yaml": "^4.0.5",
    "@types/minimatch": "^3.0.5",
    "cross-env": "7.0.3",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jest": "26.5.3",
    "eslint-plugin-prettier": "4.2.1",
    "fs-extra": "^10.1.0",
    "gitmoji-changelog": "^2.3.0",
    "husky": "8.0.1",
    "is-ci": "3.0.1",
    "jest": "27.2.5",
    "lerna": "^5.1.6",
    "lint-staged": "13.0.3",
    "npm-run-all": "4.1.5",
    "pinst": "3.0.0",
    "prettier": "2.7.1",
    "rollup": "2.75.7",
    "ts-jest": "27.0.6",
    "ts-node": "10.8.1",
    "tsconfig-paths": "4.0.0",
    "typescript": "4.7.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,eslintrc}": [
      "prettier --write"
    ]
  },
  "packageManager": "yarn@3.3.1"
}
