{
  "cSpell.words": [
    "parens"
  ],
  "code-runner.executorMap": {
    "cpp": "yarn cpp:run $fullFileName && yarn cpp:generate $fullFileName"
  },
  "C_Cpp.default.includePath": [
    "${workspaceFolder}/src",
    "/usr/include/c++/10/",
    "/usr/local/include/",
    "/usr/include/"
  ],
  "files.associations": {
    ".prettierrc": "yaml"
  }
}
